<?xml version="1.0" encoding="UTF-8"?>
<web-app id="Selenium" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>Selenium</display-name>
	<listener>
		<!--
		The following line is modified at JBoss war-building time by Ant. 
		This line is in its current state for the convenience of Eclipse/devenv.
		If you change this line, you must update the Ant build script.
		-->
		<listener-class>cbas.RegistryListener</listener-class>
	</listener>

	<servlet>
		<servlet-name>SelenifyServlet</servlet-name>
		<servlet-class>cbas.SelenifyServlet</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>SeleniumResultsServlet</servlet-name>
		<servlet-class>cbas.SeleniumResultsServlet</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>SelenifyServlet</servlet-name>
		<url-pattern>/selenify</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SeleniumResultsServlet</servlet-name>
		<url-pattern>/results</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
