<ivy-module version="2.0">
	<info organisation="joist" module="joist-migrations" revision="${joist.version}"/>
	<configurations>
		<conf name="main"/>
		<conf name="sources"/>
		<conf name="unit" extends="main" visibility="private"/>
		<conf name="build" visibility="private"/>
	</configurations>
	<publications>
		<artifact type="pom" ext="pom" conf="build"/>
		<artifact type="jar" ext="jar" conf="main"/>
		<artifact type="source" ext="jar" conf="sources"/>
	</publications>
	<dependencies>
		<dependency org="joist" name="joist-domain" rev="${joist.version}" conf="main"/>
		<dependency org="junit" name="junit" rev="3.8.2" conf="unit->master,sources"/>
	</dependencies>
</ivy-module>
